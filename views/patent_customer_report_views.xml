<?xml version="1.0" encoding="UTF-8"?>
<odoo>
     <!-- l10n_cr_land tree view -->
    <record id="l10n_cr_patent_customers_view_tree" model="ir.ui.view">
        <field name="name">l10ncr.patent.customer.report.view.tree</field>
        <field name="model">l10ncr.patent.customer.report</field>
        <field name="arch" type="xml">
            <tree create="false" delete="false">
                <field name="currency_id" invisible="1"/>
                <field name="t_identificacion" />
                <field name="titular" />
                <field name="r_legal" />
                <field name="r_identificacion" />
                <field name="t_tipo_i" />
                <field name="t_celular" />
                <field name="t_fono" />
                <field name="t_mail" />
                <field name="t_actividad" />
                <field name="direccion" />
                <field name="p_identificacion" />
                <field name="p_name" />
                <field name="p_tipo_i" />
                <field name="p_celular" />
                <field name="p_fono" />
                <field name="p_mail" />
                <field name="p_actividad" invisible="1"/>
                <field name="finca" />
                <field name="duplicado" />
                <field name="horizontal" />
                <field name="derecho" />
                <field name="provincia" />
                <field name="canton" />
                <field name="distrito" />
                <field name="direccion_f" />
                <field name="patent_id" />
                <field name="patente_tipo" />
                <field name="patente_temporalidad" invisible="1"/>
                <field name="costo_trimestral" sum="Total trimestral"/>
                <field name="aprobacion" />
                <field name="cierre" invisible="1"/>
                <field name="state" />
            </tree>
        </field>
    </record>

    <record id="l10n_cr_patent_customers_action" model="ir.actions.act_window">
        <field name="name">Lista de Patentados</field>
        <field name="res_model">l10ncr.patent.customer.report</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>
