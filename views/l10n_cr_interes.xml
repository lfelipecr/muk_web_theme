<?xml version="1.0" encoding="UTF-8"?>
<odoo>
     <!-- l10n_cr_land tree view -->
    <record id="l10n_cr_interes_view_tree" model="ir.ui.view">
        <field name="name">l10n_cr.interes.view.tree</field>
        <field name="model">l10n_cr.interes</field>
        <field name="arch" type="xml">
            <tree>
                <field name="date"/>
                <field name="rate"/>
            </tree>
        </field>
    </record>

    <record id="l10n_cr_interes_action" model="ir.actions.act_window">
        <field name="name">Intereses</field>
        <field name="res_model">l10n_cr.interes</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
